{
  "name": "termwhat",
  "version": "2.0.0",
  "description": "AI-powered terminal command suggestions with multi-provider support (Ollama, OpenAI, Anthropic, OpenRouter)",
  "main": "dist/index.js",
  "bin": {
    "termwhat": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src/",
    "test": "node --test dist/**/*.test.js",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": ["cli", "terminal", "ollama", "openai", "anthropic", "ai", "command-line", "llm"],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
    "commander": "^12.0.0",
    "ollama": "^0.5.0",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.4.0",
    "tsx": "^4.0.0"
  }
}
